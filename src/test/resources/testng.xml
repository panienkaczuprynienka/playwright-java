<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Playwright tests" parallel="classes" thread-count="5">

    <listeners>
        <listener class-name="listeners.TestStatusListener"/>
        <listener class-name="listeners.AnnotationTransformerForRetry"/>
    </listeners>

    <test thread-count="5" name="Playwright Tests">
<packages>
    <package name="tests.testngTests.*"></package>
</packages>
    </test>

</suite>